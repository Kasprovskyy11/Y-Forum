import axios from "axios";
import { postInterface } from "../services/getPostsService";

export interface UserData {
  name: string;
  username: string;
  profilePicture: string;
  birth_date: string;
  posts: postInterface[];
}

export const getUserData = async (userId: number) => {
  const response = await axios.post("http://localhost/getUserData.php", {
    id: userId,
  });
  return response.data;
};
